{
  "name": "alexandre-website",
  "version": "1.0.0",
  "description": "Alexandre Djerbetian - personal website",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "bootstrap": "yarn install --slient && yarn compile",
    "mono:update": "./monoExec.sh 'yarn update'",
    "build": "./monoRun.sh build",
    "compile": "./monoRun.sh compile",
    "clean": "lerna clean --yes && ./monoExec.sh 'rm -rf dist/'",
    "test": "./monoRun.sh test",
    "dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up --build",
    "prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up --build",
    "e2e": "./e2e.sh build && (./e2e.sh run || ./e2e.sh stop) && ./e2e.sh stop",
    "cypress": "(cd packages/e2e && yarn cypress:open)"
  },
  "dependencies": {
    "lerna": "^3.20.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-chai-friendly": "^0.6.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-vue": "^6.2.2",
    "prettier": "^2.0.5",
    "typescript": "~3.8.3"
  }
}
